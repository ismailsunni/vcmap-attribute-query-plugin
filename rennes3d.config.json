  "projection": {
    "epsg": "25833",
    "proj4": "+proj=utm +zone=33 +ellps=GRS80 +units=m +no_defs"
  },
  "i18n": [
    {
      "name": "base",
      "de": {
        "layers": {
          "pointcloud": {
            "title": "Tiergarten"
          },
          "osmBuildingFootprints": {
            "title": "OSM Geb채ude Grundfl채chen"
          }
        },
        "contentTree": {
          "building": {
            "title": "3D Geb채ude"
          }
        }
      },
      "en": {
        "layers": {
          "pointcloud": {
            "title": "Zoological garden"
          },
          "osmBuildingFootprints": {
